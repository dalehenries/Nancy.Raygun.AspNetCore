{
  "version": "1.0.1",
  "authors": [
    "dalehenries", "jdkelley", "hallesesne"
  ],
  "description": "Nancy.Raygun.AspNetCore is a non-official library, based on Nancy.Raygun, that wires up the error handling in Nancy to submit error to Raygun for dotnet core Nancy apps. See www.raygun.io for more details about the Raygun service.",
  "packOptions": {
    "owners": [
      "dalehenries", "jdkelley", "hallesesne"
    ],
    "tags": [
      "Nancy", "Raygun", "dotnet"
    ],
    "projectUrl": "https://github.com/dalehenries/Nancy.Raygun.AspNetCore",
    "licenseUrl": "https://raw.githubusercontent.com/dalehenries/Nancy.Raygun.AspNetCore/master/License.md",
    "summary": "Nancy provider for Raygun service on dotnet core"

  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Nancy": "2.0.0-clinteastwood",
        "Microsoft.Extensions.Configuration.FileExtensions": "1.0.0",
        "Microsoft.Extensions.Configuration.CommandLine": "1.0.0",
        "Microsoft.Extensions.Configuration.Json": "1.0.0",
        "System.Diagnostics.StackTrace": "4.3.0",
        "System.Diagnostics.TraceSource": "4.3.0",
        "System.Reflection.TypeExtensions": "4.3.0",
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      }
    }
  }
}
